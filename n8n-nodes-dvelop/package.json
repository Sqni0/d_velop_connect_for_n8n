{
  "name": "n8n-nodes-dvelop",
  "version": "0.1.0",
  "description": "d.velop platform nodes for n8n",
  "keywords": [
    "n8n-community-node-package"
  ],
  "license": "MIT",
  "homepage": "",
  "author": {
    "name": "",
    "email": ""
  },
  "repository": {
    "type": "git",
    "url": ""
  },
  "main": "index.js",
  "scripts": {
    "build": "tsc && gulp build:icons",
    "dev": "tsc --watch",
    "format": "prettier nodes credentials --write",
    "lint": "eslint nodes credentials package.json",
    "lintfix": "eslint nodes credentials package.json --fix",
    "prepublishOnly": "npm run build && npm run lint -s"
  },
  "files": [
    "dist"
  ],
  "n8n": {
    "n8nNodesApiVersion": 1,
    "credentials": [
      "dist/credentials/DvelopApi.credentials.js"
    ],
    "nodes": [
      "dist/nodes/DvelopPilotDocument/DvelopPilotDocument.node.js",
      "dist/nodes/DvelopPilotAssistant/DvelopPilotAssistant.node.js",
      "dist/nodes/DvelopDownloadDocument/DvelopDownloadDocument.node.js",
      "dist/nodes/DvelopGetDocumentInfo/DvelopGetDocumentInfo.node.js",
      "dist/nodes/DvelopGetUserInfo/DvelopGetUserInfo.node.js",
      "dist/nodes/DvelopStartSignature/DvelopStartSignature.node.js",
      "dist/nodes/DvelopGenerateCacheUrls/DvelopGenerateCacheUrls.node.js",
      "dist/nodes/DvelopImportDocument/DvelopImportDocument.node.js",
      "dist/nodes/DvelopGreetUser/DvelopGreetUser.node.js"
    ]
  },
  "devDependencies": {
    "@typescript-eslint/parser": "^5.45.0",
    "eslint": "^8.29.0",
    "eslint-plugin-n8n-nodes-base": "^1.11.0",
    "gulp": "^4.0.2",
    "n8n-workflow": "*",
    "prettier": "^2.7.1",
    "typescript": "^4.8.4"
  },
  "peerDependencies": {
    "n8n-workflow": "*"
  }
}

