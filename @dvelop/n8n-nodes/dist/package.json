{
    "name": "@dvelop/n8n-nodes",
    "version": "0.1.2",
    "description": "n8n community node for executing d.velop Actions",
    "license": "MIT",
    "homepage": "https://github.com/Sqni0/d_velop_connect_for_n8n/tree/main/@dvelop/n8n-nodes",
    "keywords": [
        "n8n-community-node-package",
        "n8n",
        "d.velop",
        "dms",
        "automation"
    ],
    "author": {
        "name": "Santino",
        "email": "Santino.knecht@d-velop.de"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Sqni0/d_velop_connect_for_n8n.git"
    },
    "scripts": {
        "build": "n8n-node build",
        "build:watch": "tsc --watch",
        "dev": "n8n-node dev",
        "lint": "n8n-node lint",
        "lint:fix": "n8n-node lint --fix",
        "typecheck": "tsc --noEmit --incremental false -p tsconfig.json",
        "validate:n8n-manifest": "node scripts/validate-n8n-manifest.js",
        "test": "npm run validate:n8n-manifest",
        "verify": "npm run lint && npm run typecheck && npm run test && npm run build && npm pack --dry-run",
        "release": "n8n-node release",
        "prepublishOnly": "npm run verify"
    },
    "files": [
        "dist"
    ],
    "n8n": {
        "n8nNodesApiVersion": 1,
        "credentials": [
            "dist/credentials/DvelopApi.credentials.js"
        ],
        "nodes": [
            "dist/nodes/DvelopPlatform/DvelopActions.node.js"
        ]
    },
    "devDependencies": {
        "@n8n/node-cli": "0.19.0",
        "@types/node": "^25.2.3",
        "eslint": "9.32.0",
        "prettier": "3.6.2",
        "release-it": "^19.0.4",
        "typescript": "5.9.2"
    },
    "peerDependencies": {
        "n8n-workflow": "*"
    }
}
